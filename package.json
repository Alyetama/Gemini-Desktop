{
  "name": "geminidesktop",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "node build-script.js"
  },
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.alyetama.geminidesktop",
    "productName": "Gemini Desktop",
    "publish": "git@github.com:Alyetama/Gemini-Desktop.git",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "assets/google-gemini-icon.icns",
      "target": [
        "dmg"
      ]
    }
  }
}
